super + d
    passmenu

super + v
    dmenu-pdfs

super + w
    feh --no-fehbg --bg-fill --randomize ~/.local/wallpapers/*
    
super + t
    $TERMINAL

super + e ; {e,o,u}
    st -c "st-scratch" -e $EDITOR { $HOME/.local/documents/notes/aa/tk.md,$HOME/.local/documents/notes/aa/o.md, $HOME/.local/documents/notes/aa/u.md }

super + ctrl + x
    xlock -mode blank
