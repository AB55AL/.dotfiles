    st -c "st-scratch" -e $EDITOR { \
        $HOME/.local/documents/notes/aa/tk.md, \
        $HOME/.local/documents/notes/aa/o.md, \
        $HOME/.local/documents/notes/aa/u.md, \
        $HOME/.local/documents/notes/aa/a.md \
        }

super + ctrl + x
    lockscreen.sh
